    "/auth/login": {
      "post": {
        "description": "",
        "responses": {
          "200": {
            "description": "Login response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/loginSchema"
                },
                "example": {
                  "status": true,
                  "data": {
                    "email": "johndoe@sample.com",
                    "fullname": "John Doe",
                    "phone": "08012345678"
                  },
                  "message": "Successful message"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/loginSchema"
              },
              "example": {
                "email": "johndoe@sample.com",
                "password": "password"
              }
            }
          }
        }
      }
    },
    "/auth/register": {
      "post": {
